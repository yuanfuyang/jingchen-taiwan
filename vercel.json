{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/assistant",
      "dest": "/server.js"
    },
    {
      "src": "/(imgs|style|main)\.(jpg|jpeg|png|gif|css|js)",
      "dest": "/$1.$2"
    },
    {
      "src": "/imgs/(.*)",
      "dest": "/imgs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}